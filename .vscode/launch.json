    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Attach",
            "request": "attach",
            "hostName": "localhost",
            "port": "8000"
        },
        {
            "type": "java",
            "request": "attach",
            "name": "Attach by Process ID",
            "processId": "${command:PickJavaProcess}"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "LICENSE",
            "request": "launch",
            "mainClass": "org.bouncycastle.LICENSE",
            "projectName": "mpgram3"
        },
        {
            "type": "java",
            "name": "JPAKEExample",
            "request": "launch",
            "mainClass": "org.bouncycastle.crypto.examples.JPAKEExample",
            "projectName": "mpgram3"
        },
        {
            "type": "java",
            "name": "DiscoverEndomorphisms",
            "request": "launch",
            "mainClass": "org.bouncycastle.math.ec.tools.DiscoverEndomorphisms",
            "projectName": "mpgram3"
        },
        {
            "type": "java",
            "name": "IntegerFunctions",
            "request": "launch",
            "mainClass": "org.bouncycastle.pqc.math.linearalgebra.IntegerFunctions",
            "projectName": "mpgram3"
        }
    ]
}